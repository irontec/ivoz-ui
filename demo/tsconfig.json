{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@irontec/ivoz-ui": [
        "../../library/src/"
      ],
      "@irontec/ivoz-ui/*": [
        "../../library/src/*"
      ]
    },
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}